input {
  redis {
    host => "<%= @redis_server_ip %>"
    data_type => "list"
    key => "logstash"
    codec => json
  }
  file {
    type => "syslog"
    path => ["/var/log/messages","/var/log/secure","/var/log/*.log"]
  }
}

output {
  stdout { debug => true debug_format => "json"}
  elasticsearch { host => "<%= @elasticsearch_server_ip %>" }
}

